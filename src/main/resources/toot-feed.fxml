<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml" stylesheets="@css/feed.css" styleClass="feedWrapper">
    <Label fx:id="feedLabel" styleClass="feedLabel"/>
    <ScrollPane
            fx:id="feedScroller"
            styleClass="feedWrapper"

            hbarPolicy="NEVER">
        <content>
            <VBox fx:id="tootFeedWrapper"/>
        </content>
    </ScrollPane>
</VBox>