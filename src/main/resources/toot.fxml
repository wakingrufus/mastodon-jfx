<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" stylesheets="@css/toot.css" styleClass="tootWrapper">
    <HBox fx:id="accountView"/>
    <VBox fx:id="parseContent" styleClass="tootContent"/>
    <HBox fx:id="tootActions">
        <Button fx:id="replyButton">
            <text>↰</text>
        </Button>
        <Button fx:id="favButton">
            <text>☆</text>
        </Button>
        <Button fx:id="boostButton">
            <text>♲</text>
        </Button>
    </HBox>
</VBox>